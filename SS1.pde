//3shapes by jay

void setup() {
  background(0);
  size(1080, 720);
}

void draw() {
  fill(100, 255, 95);
  stroke(255, 15, 120);
  ellipse(455, 555, 250, 350);
  
  fill(100, 60, 25);
  stroke(65, 45, 190);
  triangle(30, 175, 160, 120, 185, 170);
  
  fill(20, 255, 75);
  stroke(0, 95, 220);
  rect(500, 500, 360, 360);
}
